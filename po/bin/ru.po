msgstr "msgid определён дважды: %s"

#: ../../lib/Locale/Po4a/Po.pm:1345
#, perl-format
msgid ""
"Translations don't match for:\n"
"%s\n"
"-->First translation:\n"
"%s\n"
" Second translation:\n"
"%s\n"
" Old translation discarded."
msgstr ""
"Несоответствие переводов для:\n"
"%s\n"

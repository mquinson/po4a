#, no-wrap
msgid ""
"  \\c foo\n"
"  \\c bar\n"
"\n"
msgstr ""
"  \\c foo\n"
"  \\c bar\n"
"\n"

#. type: textblock
#: lib/Locale/Po4a/Halibut.pm:13

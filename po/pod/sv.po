" .RH              .rn             .S<             .sh             .SI\n"
" .splitfont       .Sx             .T              .TF             .The\n"
" .TT              .UC             .ul             .Vb             .zZ"
msgstr ""
" ..               .\"              .AT             .b              .bank\n"
" .BE              ..br            .Bu             .BUGS           .BY\n"
" .ce              .dbmmanage      .do                             .En\n"
" .EP              .EX             .Fi             .hw             .i\n"
" .Id              .l              .LO             .mf\n"
" .N               .na             .NF             .nh             .nl\n"

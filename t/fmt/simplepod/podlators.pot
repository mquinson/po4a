#. type: verbatim
#: podlators.pod:347
#, no-wrap
msgid ""
"    use Term::ANSIColor qw(:constants);\n"
"    print BOLD, BLUE, \"This text is in bold blue.\\n\", RESET;\n"
"\n"
"    use Term::ANSIColor qw(:constants); $Term::ANSIColor::AUTORESET = 1; "
"print BOLD BLUE \"This text is in bold blue.\\n\"; print \"This text is "
"normal.\\n\";"

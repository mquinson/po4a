#. type: verbatim
#: podlators.pod:332
#, no-wrap
msgid ""
"    use Term::ANSIColor;\n"
"    print color 'bold blue';\n"
"    print \"This text is bold blue.\\n\";\n"
"    print color 'reset';\n"
"    print \"This text is normal.\\n\";\n"
"    print colored (\"Yellow on magenta.\\n\", 'yellow on_magenta');\n"
"    print \"This text is normal.\\n\";\n"
"    print colored ['yellow on_magenta'], \"Yellow on magenta.\\n\";\n"
"\n"
"    use Term::ANSIColor qw(uncolor);\n"
"    print uncolor '01;31', \"\\n\";"
msgstr ""

#. type: textblock
#: podlators.pod:344
msgid ""
"But this isn't verbatim (make sure it wraps properly), and the next "
"paragraph is again:"
